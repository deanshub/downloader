{
    "name": "downloader",
    "version": "1.0.0",
    "description": "Torrent Download Telegram Bot",
    "main": "index.js",
    "module": "index.ts",
    "scripts": {
        "build": "npm run clean && ts-node scripts/build.ts",
        "tsc": "tsc",
        "watch": "tsc -w",
        "clean": "rm -rf build/",
        "start": "node -r dotenv/config build/index.js",
        "god": "sudo env \"PATH=$PATH\" nodemon",
        "dev": "nodemon",
        "tsnode": "ts-node src/index.ts",
        "mount": "fdisk -l && mkdir /media/external && mount /dev/sdc1 /media/external",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deanshub/downloader.git"
    },
    "keywords": [
        "telegram",
        "bot",
        "torrent"
    ],
    "author": "Dean Shub",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/deanshub/downloader/issues"
    },
    "homepage": "https://github.com/deanshub/downloader#readme",
    "dependencies": {
        "@types/node": "^20.5.1",
        "@types/webtorrent": "^0.109.3",
        "axios": "^0.21.4",
        "bytes": "^3.1.2",
        "check-disk-space": "^3.4.0",
        "debug": "^4.3.4",
        "dotenv": "^10.0.0",
        "execa": "^5.1.1",
        "fs-extra": "^10.1.0",
        "humanize-duration": "^3.29.0",
        "lru-cache": "^7.18.3",
        "parse-torrent": "^9.1.5",
        "prettysize": "^2.0.0",
        "telegraf": "^4.12.2",
        "thepiratebay": "^1.4.8",
        "toad-scheduler": "^1.6.1",
        "torrent-search-api": "^2.1.4",
        "typescript": "^4.9.5",
        "uuid": "^8.3.2",
        "webtorrent": "^1.9.7"
    },
    "devDependencies": {
        "@swc/core": "^1.3.78",
        "@swc/jest": "^0.2.29",
        "@types/bytes": "^3.1.1",
        "@types/fs-extra": "^9.0.13",
        "@types/humanize-duration": "^3.27.1",
        "@types/jest": "^27.5.2",
        "@types/thepiratebay": "^1.4.3",
        "@types/torrent-search-api": "^2.0.0",
        "@types/uuid": "^8.3.4",
        "brotli": "^1.3.3",
